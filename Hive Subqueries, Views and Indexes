#Subquery

select id from customers
WHERE NOT EXISTS
(
select customer_id from orders
where orders.customer_id = customers.id
);

hive> select id from customers
    > WHERE NOT EXISTS
    > (
    > select customer_id from orders
    > where orders.customer_id = customers.id
    > );
